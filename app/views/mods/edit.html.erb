<div class="container">
  <h1>Edit module for session</h1>

  <%= simple_form_for [@mod.session.project, @mod.session, @mod] do |f| %>
    <%= f.input :title %>
    <%= f.input :duration, input_html: { min: '0' } %>
    <%= f.input :format %>
    <%= f.input :description %>
    <%= f.submit %>
  <% end %>
</div>

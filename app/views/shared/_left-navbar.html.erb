<div class="left-navbar">
  <div class="dropdown">
    <div class="left-navbar-top">
      <%= image_tag "Yahya.png", class: "avatar dropdown-toggle", id: "navbar-wagon-menu", "data-toggle" => "dropdown" %>
      <ul class="dropdown-menu dropdown-menu-right navbar-wagon-dropdown-menu">
        <li>
          <%= link_to destroy_user_session_path, method: :delete do %>
            <i class="fa fa-sign-out"></i>  <%= t(".sign_out", default: "Log out") %>
          <% end %>
        </li>
      </ul>
    </div>
  </div>
  <div class="left-navbar-middle">
    <%= link_to projects_path, class: 'left-buttons' do %>
      <div class="left-menu-list">
        <i class="fas fa-tasks"></i> <div class="left-menu-title"><p>Projects</p></div>
      </div>
    <% end %>
    <%= link_to contents_path, class: 'left-buttons' do %>
      <div class="left-menu-list">
        <i class="fas fa-book"></i> <div class="left-menu-title"><p>Contents</p></div>
      </div>
    <% end %>
    <%= link_to clients_path, class: 'left-buttons' do %>
      <div class="left-menu-list">
        <i class="fas fa-users"></i> <div class="left-menu-title"><p>Clients</p></div>
      </div>
    <% end %>
    <%= link_to users_path, class: 'left-buttons' do %>
      <div class="left-menu-list">
        <i class="fas fa-user-tie"></i></i> <div class="left-menu-title"><p>Users</p></div>
      </div>
    <% end %>
  </div>
  <div class="left-navbar-bottom">
    <%= link_to root_path, class: "left-navbar-logo" do %>
      <%= image_tag "logo-seven-navbar.png" %>
    <% end %>
  </div>
</div>
